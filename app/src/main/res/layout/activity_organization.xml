<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
                xmlns:app="http://schemas.android.com/apk/res-auto"
                xmlns:tools="http://schemas.android.com/tools"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@color/colorDefaultBg"
                tools:context="com.cphandheld.cphmobilerec.OrganizationActivity">

    <!--<include layout="@layout/header"></include>-->

    <Button
        android:id="@+id/buttonImportData"
        android:layout_width="120dp"
        android:layout_height="36dp"
        android:layout_marginTop="20dp"
        android:padding="5dp"
        android:fontFamily="Lato Regular"
        android:background="@color/colorHeader"
        android:textColor="@android:color/white"
        android:text="Import Data"
        android:textSize="20sp"
        android:layout_marginRight="20dp"
        android:layout_marginLeft="20dp"/>


    <ListView
        android:id="@+id/listOrganizations"
        android:layout_height="wrap_content"
        android:layout_width="match_parent"
        android:layout_marginTop="10dp"
        android:padding="20dp"
        android:divider="@color/colorDefaultBg"
        android:dividerHeight="10dp"
        android:layout_below="@+id/buttonImportData"
        />

</RelativeLayout>